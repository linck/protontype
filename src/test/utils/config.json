{
  "port": "3000",
  "defaultRoutes": true,
  "database": {
    "name": "defaultTestConnection",
    "type": "sqlite",
    "database": "./src/test/proton.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
      "./dist/test/utils/**/*.js"
    ]
  },
  "cors": {
    "origin": "*",
    "methods": [
      "GET",
      "POST",
      "OPTIONS",
      "PUT",
      "PATCH",
      "DELETE"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Authorization"
    ]
  },
  "jwtSecret": "secret",
  "jwtSession": {
    "session": false
  },
  "logger": {
    "enabled": false,
    "transports": [
      {
        "type": "file",
        "options": {
          "filename": "./src/test/logs.log"
        }
      },
      {
        "type": "console"
      }
    ]
  },
  "https": {
    "enabled": false,
    "key": "./src/test/cert/cert.key",
    "cert": "./src/test/cert/cert.cert"
  }
}