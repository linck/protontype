{
  "name": "protontype",
  "version": "2.0.0-rc.4",
  "description": "",
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "build": "tsc -w",
    "start": "npm run build",
    "test": "tsc && mocha -t 30000 dist/test/**/*.js",
    "coverage": "tsc && nyc --reporter=html --reporter=text mocha -t 30000 dist/test/**/*.js",
    "gen-docs": "typedoc -out ./docs -theme node_modules/protontype-doc-theme/minimal -excludePrivate -name \"Protontype Reference API\" -readme none && touch ./docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linck/protontype"
  },
  "homepage": "https://github.com/linck/protontype",
  "keywords": [
    "typescript",
    "express",
    "sequelize",
    "API",
    "REST",
    "RESTFul"
  ],
  "author": "Humberto Machado <betolinck@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "class-transformer": "^0.1.8",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "helmet": "^3.8.0",
    "jsontyped": "^1.2.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "^0.1.2",
    "typescript-collections": "^1.2.3",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.4",
    "@types/cors": "2.8.1",
    "@types/express": "^4.0.33",
    "@types/helmet": "0.0.35",
    "@types/node": "^6.0.68",
    "@types/winston": "0.0.29",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.32",
    "@types/supertest": "^2.0.2",
    "chai": "^4.1.0",
    "concurrently": "^3.5.0",
    "mocha": "^3.1.2",
    "mocha-typescript": "^1.0.10",
    "nyc": "^10.1.2",
    "protontype-doc-theme": "^1.0.0-alpha",
    "supertest": "^3.0.0",
    "touch-cli": "0.0.1",
    "typedoc": "^0.5.10",
    "sqlite3": "^3.1.13",
    "typescript": "^2.6.1"
  },
  "nyc": {
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ]
  }
